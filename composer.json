    "laravel",
    "passport",
    "oauth"
  ],
  "require": {
    "guzzlehttp/guzzle": "^6.*|^7.*",
    "laravel/passport": "^8.*|^9.*"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "iamnotstatic",
      "email": "staticdev20046@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "Iamnotstatic\\LaravelAPIAuth\\": "src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Iamnotstatic\\LaravelAPIAuth\\LaravelAPIAuthServiceProvider"
      ]
    }
  }
